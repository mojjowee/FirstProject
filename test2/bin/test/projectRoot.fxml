      </TableView>
      <Label layoutX="200.0" layoutY="18.0" text="목록 조회" />
      <Button layoutX="132.0" layoutY="276.0" mnemonicParsing="false" onAction="#handleLend" text="대출" />
      <Button layoutX="200.0" layoutY="276.0" mnemonicParsing="false" onAction="#handleReturn" text="반납" />
      <Button layoutX="260.0" layoutY="276.0" mnemonicParsing="false" onAction="#handleExit" text="닫기" />
   </children>
